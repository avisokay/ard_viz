generate_edges = function(){
# each node knows 5 other nodes
from = list()
for(i in 1:15){
from = append(from, rep(i, 5))
}
from = unlist(from)
# 3 clusters, 1-5, 6-10 and 11-15
to = list()
for(i in 1:15){
if(i < 6){
to = append(to, c(sample(1:5, 4), sample(6:15, 1)))
} else if(i > 5 | i < 11){
to = append(to, c(sample(6:10, 4), sample(c(1:5, 11:15), 1)))
} else if(i > 10){
to = append(to, c(sample(11:15, 4), sample(1:10), 1))
}
}
to = unlist(to)
edges = data.frame(from, to) %>%
filter(to != from) # remove self loops
return(edges)
}
set.seed(2)
